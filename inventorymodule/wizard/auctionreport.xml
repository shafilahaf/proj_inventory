<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="ae_auction_report_wizard_view_form" model="ir.ui.view">
        <field name="name">ae.auction.report.wizard.view.form</field>
        <field name="model">ae.inventory.auction.report.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="date_filter" />                    
                    </group>
                </sheet>
                <footer>
                    <button name="action_print_report" string="Print" type="object" class="oe_highlight" />
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

</odoo>

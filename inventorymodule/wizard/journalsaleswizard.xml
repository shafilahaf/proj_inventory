<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="journal_sales_view_form" model="ir.ui.view">
        <field name="name">journal.sales.view.form</field>
        <field name="model">ae.itemjournal.line.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="inv_led_en" context="{'tree_view_ref': 'inventorymodule.ae_inv_ledger_view_tree_salesreturn'}" string="Sales Data"/>
                    </group>
                </sheet>
                <footer>
                    <button name="insert_item_journal_line" string="Insert To Line" type="object" class="oe_highlight" />
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

</odoo>

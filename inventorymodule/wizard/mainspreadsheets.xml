<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="ae_mainspreadsheet_report_wizard_view_form" model="ir.ui.view">
        <field name="name">ae.mainspreadsheet.report.wizard.view.form</field>
        <field name="model">ae.inventory.main.spreadsheets.reports.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="ae_location" options="{'no_create': True, 'no_create_edit':True}"/>
                        <field name="ae_asset_number" />
                        <field name="ae_inventory_group" options="{'no_create': True, 'no_create_edit':True}"/>
                    </group>
                </sheet>
                <footer>
                    <button name="action_print_report" string="Print" type="object" class="oe_highlight" />
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

</odoo>
